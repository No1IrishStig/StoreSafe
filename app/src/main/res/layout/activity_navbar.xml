<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/navContainer"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:layout_marginTop="676dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/navVaultIcon"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="318dp"
        android:background="@drawable/Safe"
        app:layout_constraintBottom_toBottomOf="@+id/navContainer"
        app:layout_constraintEnd_toEndOf="@+id/navContainer"
        app:layout_constraintStart_toStartOf="@+id/navContainer"
        app:layout_constraintTop_toTopOf="@+id/navContainer" />

    <TextView
        android:id="@+id/navGenerateIcon"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginStart="178dp"
        android:layout_marginEnd="178dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/Generate"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/navContainer"
        app:layout_constraintEnd_toEndOf="@+id/navContainer"
        app:layout_constraintStart_toStartOf="@+id/navContainer"
        app:layout_constraintTop_toTopOf="@+id/navContainer" />

    <TextView
        android:id="@+id/navCogIcon"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginStart="316dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/Cog"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/navContainer"
        app:layout_constraintEnd_toEndOf="@+id/navContainer"
        app:layout_constraintStart_toStartOf="@+id/navContainer"
        app:layout_constraintTop_toTopOf="@+id/navContainer" />

    <Button
        android:id="@+id/navVault"
        android:layout_width="135dp"
        android:layout_height="56dp"
        android:layout_marginEnd="272dp"
        android:background="null"
        android:style="?android:attr/borderlessButtonStyle"
        app:layout_constraintBottom_toBottomOf="@+id/navContainer"
        app:layout_constraintEnd_toEndOf="@+id/navContainer"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/navContainer"
        app:layout_constraintTop_toTopOf="@+id/navContainer"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/navGenerate"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginStart="135dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="1dp"
        android:background="null"
        android:style="?android:attr/borderlessButtonStyle"
        app:layout_constraintBottom_toBottomOf="@+id/navContainer"
        app:layout_constraintEnd_toEndOf="@+id/navContainer"
        app:layout_constraintStart_toStartOf="@+id/navContainer"
        app:layout_constraintTop_toTopOf="@+id/navContainer" />

    <Button
        android:id="@+id/navSettings"
        android:layout_width="136dp"
        android:layout_height="55dp"
        android:layout_marginStart="275dp"
        android:layout_marginTop="1dp"
        android:background="null"
        android:style="?android:attr/borderlessButtonStyle"
        app:layout_constraintBottom_toBottomOf="@+id/navContainer"
        app:layout_constraintEnd_toEndOf="@+id/navContainer"
        app:layout_constraintStart_toStartOf="@+id/navContainer"
        app:layout_constraintTop_toTopOf="@+id/navContainer" />

</androidx.constraintlayout.widget.ConstraintLayout>