<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.core.storesafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ViewSites_Activity"></activity>
        <activity android:name=".NewCredentialsHolder" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".GenerateActivity" />
        <activity
            android:name=".ForgottenActivity"
            android:label="Password Reset" />
        <activity
            android:name=".LoginActivity"
            android:label="Login" />
        <activity
            android:name=".HomeActivity"
            android:label="Home" />
        <activity
            android:name=".MainActivity"
            android:label="StoreSafe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>